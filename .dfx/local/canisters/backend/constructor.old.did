service : {
  checkFileExists: (name: text) -> (bool);
  getFiles: () ->
   (vec
     record {
       fileType: text;
       hash: text;
       name: text;
       size: nat;
       timestamp: int;
     });
  uploadFile: (name: text, content: blob, fileType: text) -> (text);
  verifyFileByHash: (hash: text) ->
   (opt record {
          fileType: text;
          name: text;
          owner: principal;
          timestamp: int;
        });
}
